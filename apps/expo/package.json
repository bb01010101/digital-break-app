{
  "name": "@acme/expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "export": "expo export --platform web",
    "dev:client": "npx expo start --clear --dev-client",
    "lint": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "generate-themes": "tamagui generate-themes ./theme/theme-builder.ts ./theme/theme-output.ts",
    "build:development": "eas build --profile development --platform ios --local",
    "build:simulator": "eas build --profile simulator --platform ios --local",
    "build:production": "eas build --profile production --platform ios --local",
    "prebuild": "expo prebuild --clean -p ios",
    "analyze:ios": "source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@acme/target-plugin": "workspace:*",
    "@expo/vector-icons": "^15.0.3",
